{
    "name": "system",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "format": "prettier --single-quote --trailing-comma all --write --tab-width 4 '**/*.js'",
        "prepare": "husky install"
    },
    "husky": {
