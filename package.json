        "debug": "~2.6.9",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.8",
        "email-validator": "^2.0.4",
        "excel4node": "^1.7.2",
        "express": "~4.16.1",
        "fs": "^0.0.1-security",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "mime": "^1.6.0",
        "mongoose": "^6.2.9",
        "mongoose-delete": "^0.5.4",
        "multer": "^1.4.4",
        "querystring": "^0.2.1",
        "read-excel-file": "^5.2.28",
        "swagger-autogen": "^2.22.0",
        "swagger-ui-express": "^4.5.0",
        "uuid": "^8.3.2",
        "validate-phone-number-node-js": "^0.0.1"
    },
    "engines": {
        "node": "16.x"
    },
    "devDependencies": {
        "eslint": "^8.26.0",
